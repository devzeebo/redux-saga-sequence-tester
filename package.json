{
  "name": "redux-saga-sequence-testing",
  "version": "0.0.1",
  "description": "Test Redux Sagas by stepping through them with inline returns and automatic calls",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "files": [
    "lib",
    "src",
    "!**/*.spec.ts",
    "!**/*.spec.d.ts",
    "!**/*.spec.js.map",
    "!**/*.spec.js"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc --project ./tsconfig.json --outDir ./lib",
    "test": "jest",
    "lint": "eslint ./src",
    "test:coverage": "npm run test -- --coverage --roots './src'",
    "prepublishOnly": "run-s clean build test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/devzeebo/redux-saga-sequence-testing.git"
  },
  "keywords": [
    "redux",
    "saga",
    "test",
    "testing",
    "react"
  ],
  "author": "Eric Siebeneich <eric.siebeneich@gmail.com> (https://github.com/devzeebo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devzeebo/redux-saga-sequence-testing/issues"
  },
  "homepage": "https://github.com/devzeebo/redux-saga-sequence-testing#readme",
  "peerDependencies": {
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@jest/globals": "^27.3.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "axios": "^0.24.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-watch": "^7.0.0",
    "jest": "^27.3.1",
    "jest-gwt": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "redux-saga": "^1.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typed-redux-saga": "^1.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "tslib": "^2.3.1"
  }
}